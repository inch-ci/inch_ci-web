#users_show= render "show"

- if !@projects_without_badges.blank?
  :javascript
    setTimeout(function() {
      jQuery.ajax(#{url_for(params.merge(:format => :js)).to_json});
    }, 4000)

- if can_edit? && @user.last_synced_projects_at.nil?
  :javascript
    I.user.syncProjects(#{sync_projects_url(protocol: 'https').to_json});
